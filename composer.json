  "type": "library",
  "keywords": [
    "stream",
    "java stream api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "HervÃ© Guenot",
      "email": "herve@guenot.info"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1.3",
    "bentools/iterable-functions": "~1.3"
  },
  "autoload": {
    "psr-4": {
      "phpstream\\": "src/phpstream"
    }
